def wow(a):
    return a * 3
    
a = [i for i in range(1, 10)]  # 1~9 값의 리스트 생성

aa = map(wow, a) # map 함수 이터레이터 값으로 반환하므로 이터레이터는 for문 사용
aa = list(map(wow, a)) # 또는 list로 변환
print(aa)

bb = list(filter(lambda x:x<7, aa))  # 이터레이터 값으로 반환하므로 list로 변환
print(bb)

# 결과 
#  [3, 6, 9, 12, 15, 18, 21, 24, 27]
#  [3, 6]
